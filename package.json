{
  "name": "@swedbank-pay/sdk",
  "version": "1.0.0",
  "description": "TypeScript / JavaScript Software Development Kit for the Swedbank Pay API Platform",
  "main": "build/src/main.js",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "/build"
  ],
  "engines": {
    "node": ">=14"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SwedbankPay/swedbank-pay-sdk-js.git"
  },
  "eslintConfig": {
    "extends": "@bjerk/eslint-config"
  },
  "prettier": "@cobraz/prettier",
  "devDependencies": {
    "@bjerk/eslint-config": "^2.2.2",
    "@cobraz/prettier": "^2.0.0",
    "@types/babel__core": "^7.1.19",
    "@types/jest": "^27.4.1",
    "@types/node": "~12.20.0",
    "@typescript-eslint/eslint-plugin": "~5.38.1",
    "@typescript-eslint/parser": "~5.38.1",
    "eslint": "~8.24.0",
    "eslint-config-prettier": "~8.5.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "~27.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.0.1",
    "eslint-plugin-unicorn": "43.0.2",
    "jest": "~29.1.1",
    "nock": "^13.2.9",
    "prettier": "~2.7.1",
    "rimraf": "~3.0.2",
    "ts-jest": "~29.0.2",
    "tsutils": "~3.21.0",
    "typedoc": "^0.22.0",
    "typedoc-plugin-markdown": "^3.1.1",
    "typescript": "~4.0.0"
  },
  "scripts": {
    "clean": "rimraf coverage build tmp",
    "generate-docs": "typedoc --mode file --readme none --plugin typedoc-plugin-markdown --hideSources --out docs src",
    "build": "tsc -p tsconfig.release.json",
    "build:watch": "tsc -w -p tsconfig.release.json",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write . --ignore-path .gitignore",
    "format:check": "prettier --check . --ignore-path .gitignore",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "author": "Swedbank Pay <opensource@swedbankpay.com>",
  "license": "MIT",
  "dependencies": {
    "@types/mocha": "^9.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "got": "^11.0.2",
    "tslib": "~2.4.0"
  }
}
