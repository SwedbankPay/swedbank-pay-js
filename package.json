{
  "name": "@swedbank-pay/sdk",
  "version": "1.0.0",
  "description": "TypeScript / JavaScript Software Development Kit for the Swedbank Pay API Platform",
  "main": "build/src/main.js",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "/build"
  ],
  "engines": {
    "node": ">= 12.13 <13"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SwedbankPay/swedbank-pay-sdk-js.git"
  },
  "devDependencies": {
    "@types/jest": "~26.0.3",
    "@types/node": "~12.20.0",
    "@typescript-eslint/eslint-plugin": "~2.34.0",
    "@typescript-eslint/parser": "~2.34.0",
    "eslint": "~6.8.0",
    "eslint-config-prettier": "~6.15.0",
    "eslint-plugin-jest": "~25.2.0",
    "jest": "~25.5.0",
    "nock": "^13.0.0",
    "prettier": "~2.3.0",
    "rimraf": "~3.0.2",
    "ts-jest": "~25.5.0",
    "tsutils": "~3.21.0",
    "typedoc": "^0.20.5",
    "typedoc-plugin-markdown": "^3.1.1",
    "typescript": "~3.9.2"
  },
  "scripts": {
    "clean": "rimraf coverage build tmp",
    "generate-docs": "typedoc --mode file --readme none --plugin typedoc-plugin-markdown --hideSources --out docs src",
    "build": "tsc -p tsconfig.release.json",
    "build:watch": "tsc -w -p tsconfig.release.json",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "author": "Bjerk AS <office@bjerk.io>",
  "license": "MIT",
  "dependencies": {
    "got": "^11.0.2",
    "tslib": "~2.3.0"
  }
}
